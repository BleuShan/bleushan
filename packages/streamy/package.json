{
  "name": "@bleushan/streamy",
  "version": "0.1.0",
  "description": "Iterator utilities",
  "keywords": [
    "Iterator",
    "Functionnal"
  ],
  "author": "Philippe Lafontaine <lafontaine.philippe@gmail.com>",
  "homepage": "https://github.com/BleuShan/bleushan#readme",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BleuShan/bleushan.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf lib",
    "compile": "babel src -d lib --ignore '**/__tests__,**/__mocks__,**/__fixtures__'",
    "lint": "eslint --fix src",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "~7.6.4",
    "@bleushan/babel-preset-env": "^3.0.1",
    "@bleushan/eslint-config": "^2.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "jest-chain": "^1.1.2",
    "jest-extended": "^0.11.2",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ],
    "transform": {
      "^.+\\.jsx?$": "../../scripts/jest/root-babel-jest.js"
    }
  },
  "release": {
    "branch": "master"
  }
}
