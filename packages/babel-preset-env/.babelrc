{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": {
          "node": "current"
        },
        "shippedProposals": true
      }
    ]
  ],
  "env": {
    "production": {
      "presets": [
        [
          "minify",
          {
            "keepFnName": true,
            "keepClassName": true
          }
        ]
      ]
    }
  }
}
