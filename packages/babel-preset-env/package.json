{
  "name": "@bleushan/babel-preset-env",
  "version": "4.0.0",
  "description": "A slightly more complicated babel preset env",
  "main": "lib/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BleuShan/bleushan.git"
  },
  "author": "Philippe Lafontaine <lafontaine.philippe@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf lib",
    "compile": "babel src -d lib --ignore '**/__tests__,**/__mocks__,**/__fixtures__'",
    "lint": "eslint --fix src",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/helper-plugin-utils": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "babel-plugin-transform-imports": "^1.5.1",
    "babel-preset-minify": "^0.5.0"
  },
  "peerDependencies": {
    "core-js": "^3.3.4",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@bleushan/babel-eslint": "^10.0.4",
    "@bleushan/eslint-config": "^3.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "core-js": "^3.3.4",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "jest-chain": "^1.1.2",
    "jest-extended": "^0.11.2",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ],
    "transform": {
      "^.+\\.jsx?$": "../../scripts/jest/root-babel-jest.js"
    },
    "setupFilesAfterEnv": [
      "jest-extended",
      "jest-chain"
    ]
  },
  "release": {
    "branch": "master"
  }
}
