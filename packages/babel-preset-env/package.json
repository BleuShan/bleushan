{
  "name": "@bleushan/babel-preset-env",
  "version": "5.0.0",
  "description": "A slightly more complicated babel preset env",
  "main": "lib/cjs/preset.js",
  "module": "lib/esm/preset.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BleuShan/bleushan.git"
  },
  "author": "Philippe Lafontaine <lafontaine.philippe@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf lib",
    "compile": "rollup --config",
    "lint": "eslint --fix src",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/helper-plugin-utils": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-decorators": "^7.7.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-private-methods": "^7.6.0",
    "@babel/plugin-syntax-bigint": "^7.4.4",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime-corejs3": "^7.7.2",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-preset-minify": "^0.5.1",
    "core-js": "^3.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@bleushan/babel-eslint": "^10.0.4",
    "@bleushan/eslint-config": "^3.1.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "jest-chain": "^1.1.2",
    "jest-extended": "^0.11.2",
    "jsverify": "0.8.4",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.2",
    "rollup": "^1.26.4",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ],
    "setupFilesAfterEnv": [
      "jest-extended",
      "jest-chain"
    ]
  },
  "release": {
    "branch": "master"
  }
}
