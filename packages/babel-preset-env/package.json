{
  "name": "@bleushan/babel-preset-env",
  "version": "1.0.0",
  "description": "A slightly more complicated babel preset env",
  "main": "lib/index.js",
  "module": "src/index.js",
  "jsnext:main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/BleuShan/bleushan.git"
  },
  "author": "Philippe Lafontaine <lafontaine.philippe@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf lib",
    "compile": "babel src -d lib --ignore \"**/__@(tests|mocks|fixtures)__\"",
    "lint": "eslint --fix src",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/helper-plugin-utils": "^7.0.0-beta.44",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0-beta.44",
    "@babel/polyfill": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-import": "^1.6.7",
    "babel-preset-minify": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@bleushan/eslint-config": "^1.0.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fixtures__/"
    ]
  },
  "release": {
    "branch": "master"
  }
}
